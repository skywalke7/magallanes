<div id="formm">
					<span class="titleGetInTouch">Escribenos</span>
					<form method="POST" action="#" id="getInTouch">
						<table border="0">
							<tbody>
								<tr>
									<td>Nombre (*)</td>
									<td>Apellido(s) (*)</td>	
								</tr>
								<tr>
									<td>
										<input type="text" value size="20" maxlength="20" name="FormName" id="name">
									</td>
									<td>
										<input type="text" value size="20" maxlength="20" name="FormLastName" id="lastName">
									</td>
								</tr>
								<tr>
									<td>Email (*)</td>
									<td>Telefono</td>	
								</tr>
								<tr>
									<td>
										<input type="text" value size="45" maxlength="50" name="FormEmail" id="email">
									</td>
									<td>
										<input type="text" value size="20" maxlength="22" name="FormPhone" id="phone">
									</td>
								</tr>
								<tr>
									<td colspan="2">Mensaje (*)</td>
								</tr>
								<tr>
									<td colspan="2"><textarea cols="50" rows="5" name="FormMessage" id="message"></textarea></td>
								</tr>
								<tr>
									<td><input type="submit" id="submitBtngetInTouch" value="Enviar"></td>
								</tr>
							</tbody>
						</table>
					</form>
				</div>